{
  "manifest_version": 2,
  "name": "Tab-U-Later",
  "version": "0.0.0.1",
  "permissions": [
    "tabs", "declarativeContent", "storage"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "browser_action": {
    "default_icon": "icon_8.png",
    "default_popup": "popup.html"
}
}
