{
  "manifest_version": 2,
  "name": "Tab-U-Later",
  "version": "0.0.0.1",

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs", "declarativeContent", "storage"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "options_page": "options.html",
  "page_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon_8.png",
      "32": "images/icon_8.png",
      "48": "images/icon_8.png",
      "128": "images/icon_8.png"
    }
  }
}
